<?xml version="1.0" encoding="UTF-8"?>
<beans 	xmlns="http://www.springframework.org/schema/beans"
		xmlns:p="http://www.springframework.org/schema/p"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
		default-init-method="turnOnLights"
		default-destroy-method="turnOffLights"
		>
		
	<bean id="guitar" class="spring.pag048.wiringCollections.Guitar"/>
	<bean id="cymbal" class="spring.pag048.wiringCollections.Cymbal"/>
	<bean id="harmonica" class="spring.pag048.wiringCollections.Harmonica"/>	
	<bean id="hank" class="spring.pag049.wiringMapCollections.OneManBand">
		<property name="instruments">
			<map>
				<entry key="GUITAR" value-ref="guitar"/>
				<entry key="CYMBAL" value-ref="cymbal"/>
				<entry key="HARMONICA" value-ref="harmonica"/>
			</map>
		</property>
	</bean>
</beans>